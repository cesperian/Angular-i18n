{
  "xliffmergeOptions": {
    "srcDir": ".",
    "genDir": "./public/i18n",
    "i18nFile": "messages.xlf",
    "i18nBaseFile": "messages",
    "i18nFormat": "xlf",
    "encoding": "UTF-8",
    "defaultLanguage": "en-US",
    "languages": ["en-US", "de", "fr"],
    "removeUnusedIds": true,
    "supportNgxTranslate": true,
    "ngxTranslateExtractionPattern": "@@|ngx-translate",
    "useSourceAsTarget": true,
    "targetPraefix": "",
    "targetSuffix": "",
    "beautifyOutput": true,
    "allowIdChange": false,
    "autotranslate": true,
    "apikeyfile": "./keys/googleTranslateApiKey.txt",
    "verbose": false,
    "quiet": false
  }
}